        <script><![CDATA[function openDeactivateCustomFieldMappingsDialog() {
	if (!g_list.getChecked()) {
		return;
	} else {
		var itemsCheckedArray = g_list.getChecked().split(',');
		var gm = new GlideModal('x_pd_integration_custom_field_mapping_deactivate_multiple_dialog');
		gm.setPreference('ids', g_list.getChecked());
		gm.setPreference('total', itemsCheckedArray.length);
		gm.setTitle('Deactivate ' + itemsCheckedArray.length + ' field mapping(s)?');
		gm.setWidth(700);
		gm.render();
	}
}]]></script>
