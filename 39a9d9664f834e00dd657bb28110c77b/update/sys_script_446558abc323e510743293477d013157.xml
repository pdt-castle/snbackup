        <script><![CDATA[(function executeRule (current, previous /* null when async */) {
  var currentUserSysId = gs.getUserID();
  var eventName;

  var whVersion = parseInt(current.getValue('webhook_version'), 10);
  switch (whVersion) {
    case 2:
      eventName = 'x_pd_integration.update_v2_wh_extension';
      break;
    case 3:
      eventName = 'x_pd_integration.update_v3_wh_subs';
      break;
    // Add cases for additional webhook versions as needed
    default:
      gs.warn('Unrecognized webhook version: ' + whVersion);
      return;
  }

  if (eventName) {
    gs.eventQueue(eventName, current, currentUserSysId, '', '');
  }
})(current, previous);
]]></script>
