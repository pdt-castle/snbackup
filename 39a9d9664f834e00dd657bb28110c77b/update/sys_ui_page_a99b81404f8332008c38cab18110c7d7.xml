        <client_script>function runTest() {
	getPayloadFieldValue();
}

function getPayloadFieldValue() {
	var ga = new GlideAjax('x_pd_integration.PagerDutyInboundFieldRuleClient');
	ga.addParam('sysparm_name', 'getValueFromPayload');
	// Set parameters for include script
	ga.addParam('sysparm_payload_field',  gel('dial_payload_field').value);
	ga.addParam('sysparm_payload', gel('dial_payload').value);
	ga.addParam('sysparm_target_field', g_form.getValue('target_field'));
	ga.addParam('sysparm_number', g_form.getValue('number'));
	ga.getXML(processResponse);
}

function processResponse(response) {
	var results = response.responseXML.getElementsByTagName('results');
	gel('dial_value').value = results[0].getAttribute('value');
	gel('dial_msg').value = results[0].getAttribute('msg');
}</client_script>
