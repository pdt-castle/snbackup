        <script>var FetchResponders = new FetchResponders();
var result = FetchResponders.fetchResponders(null);

if (result.error) {
  gs.addErrorMessage(result.error);
} else {
  if (result.warnings.length &gt; 0) {
    gs.addInfoMessage('Warnings: ' + result.warnings.join('. '));
