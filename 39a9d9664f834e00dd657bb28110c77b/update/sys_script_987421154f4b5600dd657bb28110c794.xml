        <script>function onBefore(current, previous) {

  var epHelper = new x_pd_integration.EscalationPolicyHelper();
  var pdEpId = epHelper.getPDEscalationPolicyId(current.getValue('assignment_group'));
  if (pdEpId)
    return;

  gs.debug("Incident assigned to a non-PagerDuty group and property set to unlink incident, this will also resolve the related PagerDuty incident ({0})", current.x_pd_integration_incident);
  current.x_pd_integration_incident = "---unlinked---";
}</script>
