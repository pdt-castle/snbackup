        <name>Provision Webhook into PagerDuty (Group)</name>
        <onclick>provisionWebhook()</onclick>
        <order>100</order>
        <script><![CDATA[var pd = new x_pd_integration.PagerDuty();
var pdp = new x_pd_integration.PagerDutyProvisioning();
var pdServiceId = String(current.x_pd_integration_pagerduty_service);
var webhookId = pd.searchWebhookByServiceId(pdServiceId);

if (webhookId) {
  pd.updateWebhookId(current, pdServiceId, webhookId);
  var grPdWh = new GlideRecordSecure('x_pd_integration_pagerduty_webhooks');
  if (grPdWh.get('sn_entity_sys_id', current.getValue('sys_id'))) {
    grPdWh.setValue('webhook_version', 3);
    grPdWh.update();
  }
  gs.addInfoMessage('Webhook subscription already exists for the service. PagerDuty webhook field has been updated accordingly');
} else {
  pd.provisionWebhook(current, pdServiceId);
  gs.addInfoMessage('Webhook subscription has been created');
}
]]></script>
