        <script>function addRespondersDialog() {
	ScriptLoader.getScripts('x_pd_integration.AjaxHelper.jsdbx?v='+new Date().getTime(), function() {
	var trackingApi = x_pd_integration.AjaxHelper.ajaxClientFor('TrackingController');
          trackingApi.track('incident_ui_action_clicked_add_responders_button', {},
              function(error, _result) { if (error) jslog('Tracking error: ' + error); });
          });
	var snIncidentId = g_form.getUniqueValue();
	var gm = new GlideModal('x_pd_integration_add_responders_dialog');
    gm.setPreference('snIncidentId', snIncidentId);
	gm.setTitle('Add Responders to Incident');
    gm.setWidth(700);
    gm.render();
}
