        <script><![CDATA[function runIncidentWorkflowDialog() {
  ScriptLoader.getScripts('x_pd_integration.AjaxHelper.jsdbx?v='+new Date().getTime(), function() {
    var trackingApi = x_pd_integration.AjaxHelper.ajaxClientFor('TrackingController');
    trackingApi.track('incident_ui_action_form_button_run_workflow', {},
      function(error, _result) { if (error) jslog('Tracking error: ' + error); });
  });
  var snIncidentId = g_form.getUniqueValue();
  var gm = new GlideModal('x_pd_integration_run_workflow_dialog');
  gm.setPreference('snIncidentId', snIncidentId);
  gm.setTitle('Run a Workflow');
  gm.setWidth(700);
  // Remove focus from the close button after a modal body is fully rendered (default GlideModal behaviour)
  gm.on('bodyrendered', function() {
    document.querySelector('#x_pd_integration_run_workflow_dialog_closemodal').blur();
  });
  gm.render();
}]]></script>
