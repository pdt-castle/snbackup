        <name>Provision PD Change Event Integration</name>
        <onclick/>
        <order>900</order>
        <script>gs.info("I'm in the Provision PD Change Event Integration now....");
var me = "Provision PD Change Event Integration";

var pdce = new x_pd_integration.PagerDuty_Change_Events();
var ciPdServiceId = String(current.x_pd_integration_pagerduty_service);
var ciName = String(current.name);
var ciSysId = String(current.sys_id);

gs.info('{0} the service name is: {1}, the service id is: {2}', me, ciName, ciPdServiceId);

// pass service name and service id to create integration on PD service
var createCE = pdce.createChangeEventIntegration(ciName, ciPdServiceId);

gs.info('{0} the response from createCE is: {1}', me, createCE);

// get response, and update the CI field with token
var grCI = new GlideRecord('cmdb_ci');
grCI.addQuery('sys_id', ciSysId);
grCI.query();
while (grCI.next()) {
  grCI.x_pd_integration_pagerduty_change_event_token = createCE;
  grCI.update();
}

gs.info('{0} added integration token for ci {1} with service: {2} and id: {3}', me, ciSysId, ciName, ciPdServiceId);
</script>
