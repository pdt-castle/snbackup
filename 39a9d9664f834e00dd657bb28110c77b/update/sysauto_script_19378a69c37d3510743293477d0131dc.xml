        <script><![CDATA[gs.info('Start PagerDuty custom fields metadata synchronization');
var cfSupport = new x_pd_integration.PagerDutyCustomFieldsSupport();
// Check if the account has the custom field mapping ability and update enable_custom_field_mapping system property.
cfSupport.checkCustomFieldMappingAbility();
var enableCustomFieldMapping = gs.getProperty('x_pd_integration.enable_custom_field_mapping');
if (enableCustomFieldMapping === 'true') {
  var result = cfSupport.syncCustomFieldsMetadata();

  if (result.error) {
    gs.error(result.error);
  } else if (result.message) {
    gs.info(result.message);
  }
}
]]></script>
